{
    "name": "Metrics Sentinel",
    "description": "On your's privacy guard",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/192.png",
        "32":"icons/256.png",
        "48":"icons/384.png",
        "128":"icons/512.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "webRequest"
    ],
    "host_permissions": [
        "*://*"
      ],
    "background":{}
}